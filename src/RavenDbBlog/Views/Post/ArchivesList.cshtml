@model IList<RavenDbBlog.Indexes.PostCountByMonth>
@{
    Layout = "";    
}
<ul>
    @foreach (var postCount in Model)
    {
        <li><a href="@Url.Action("Archive", "Post", new { postCount.Year, postCount.Month })"> @(new DateTime(postCount.Year, @postCount.Month,1).ToString("MMMM, yyyy")) </a> (@postCount.Count)</li>
    }
</ul>
@model IList<RaccoonBlog.Web.Indexes.PostCountByMonth>
@{
    Layout = "";    
}
<ul>
    @foreach (var postCount in Model)
    {
        <li><a href="@Url.Action("ArchiveYearMonth", "Post", new { postCount.Year, postCount.Month })"> @(new DateTime(postCount.Year, @postCount.Month,1).ToString("MMMM, yyyy", System.Globalization.CultureInfo.InvariantCulture)) </a> (@postCount.Count)</li>
    }
</ul>